{
  "name": "@jorgenevens/rest-store",
  "version": "0.1.1",
  "description": "Represent REST resources in stores such as Redux or Vuex",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm -s run test:lint && npm -s run test:unit",
    "test:lint": "eslint src",
    "test:unit": "NODE_ENV=test nyc mocha",
    "dev": "nodemon --delay 1 --exec 'npm test'",
    "build": "babel -d lib src",
    "prepare": "npm -s run test && npm -s run build"
  },
  "keywords": [
    "vuex",
    "rest",
    "api",
    "vuex",
    "resources"
  ],
  "author": "Jorgen Evens <jorgen@evens.eu>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "updeep": "^1.0.0"
  }
}
